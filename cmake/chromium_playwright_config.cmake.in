# Chromium Playwright Config
@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Find dependencies
find_dependency(nlohmann_json REQUIRED)
find_dependency(spdlog REQUIRED)

# Include the targets file
include("${CMAKE_CURRENT_LIST_DIR}/chromium_playwright_targets.cmake")

# Set variables
set(CHROMIUM_PLAYWRIGHT_VERSION @PROJECT_VERSION@)
set(CHROMIUM_PLAYWRIGHT_INCLUDE_DIRS "@PACKAGE_INCLUDE_INSTALL_DIR@")
set(CHROMIUM_PLAYWRIGHT_LIBRARIES chromium_playwright_core)

# Check if all required components have been found
check_required_components(chromium_playwright)

